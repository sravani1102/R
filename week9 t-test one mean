#t-test for one mean
data <- c(45,46,50,52,48,47,49,53,51)
alpha <- 0.01
n <- length(data)
mu <- 47.5
mean <- sum(data)/n
print(paste("Mean :",mean))
variance <- sum((data-mean)**2)/(n-1)
s <- sqrt(variance)
print(paste("Standard Deviation :",round(s,digits = 5)))
result <- t.test(data,mu=mu)
print("Calculated Value")
print(result)
tablevalue <- qt(1-(alpha/2),df=n-1)
print(paste("Table Value for two tailed test:",round(tablevalue,digits = 5)))
if(result[1]<tablevalue){
  print("H0 is accepted.")
}else{
  print("H0 is rejected.")
}
